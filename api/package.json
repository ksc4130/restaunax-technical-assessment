    "@swc/helpers": "^0.5.17",
    "@tsed/ajv": "^8.9.5",
    "@tsed/barrels": "^6.4.2",
    "@tsed/core": "^8.9.5",
    "@tsed/di": "^8.9.5",
    "@tsed/engines": "^8.9.5",
    "@tsed/exceptions": "^8.9.5",
    "@tsed/json-mapper": "^8.9.5",
    "@tsed/logger": "^7.0.2",
    "@tsed/openspec": "^8.9.5",
    "@tsed/passport": "^8.9.5",
    "@tsed/platform-cache": "^8.9.5",
    "@tsed/platform-exceptions": "^8.9.5",
    "@tsed/platform-express": "^8.9.5",
    "@tsed/platform-http": "^8.9.5",
    "@tsed/platform-log-request": "^8.9.5",
    "@tsed/platform-middlewares": "^8.9.5",
    "@tsed/platform-params": "^8.9.5",
    "@tsed/platform-response-filter": "^8.9.5",
    "@tsed/platform-views": "^8.9.5",
    "@tsed/schema": "^8.9.5",
